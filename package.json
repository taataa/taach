{
  "name": "taach",
  "version": "2.0.0-alpha.1",
  "description": "Touch input gesture library for Taaspace.",
  "keywords": [
    "touch",
    "transformation",
    "user interface"
  ],
  "homepage": "https://github.com/taataa/taach",
  "main": "index.js",
  "author": {
    "name": "Akseli Palen",
    "email": "akseli.palen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/taataa/taach.git"
  },
  "license": "MIT",
  "dependencies": {
    "component-emitter": "^1.2.1",
    "nudged": "^1.2.0"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "jshint": "^2.9.4",
    "mochify": "^2.18.1",
    "semver": "^5.3.0",
    "watch": "^1.0.1"
  },
  "peerDependencies": {
    "taaspace": "taataa/taaspace#development"
  },
  "scripts": {
    "start": "ifconfig | grep inet && python3 -m http.server 8000",
    "test": "npm run test:lint && npm run test:phantom",
    "test:phantom": "jshint test/*.js && mochify",
    "test:lint": "jshint index.js lib/*.js",

    "test:examples-minimal": "jshint examples/minimal/src/*.js",
    "build:examples-minimal": "npm run test:examples-minimal && browserify examples/minimal/src/index.js -o examples/minimal/app.js && echo 'Build finished.'",
    "watch:examples-minimal": "watch 'npm run build:examples-minimal' examples/minimal/src lib",

    "test:examples-two": "jshint examples/two/src/*.js",
    "build:examples-two": "npm run test:examples-two && browserify examples/two/src/index.js -o examples/two/app.js && echo 'Build finished.'",
    "watch:examples-two": "watch 'npm run build:examples-two' examples/two/src lib",

    "test:examples-view": "jshint examples/view/src/*.js",
    "build:examples-view": "npm run test:examples-view && browserify examples/view/src/index.js -o examples/view/app.js && echo 'Build finished.'",
    "watch:examples-view": "watch 'npm run build:examples-view' examples/view/src lib"
  }
}
